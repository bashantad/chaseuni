<div class="row">
	<div class="col-xs-3">
		<h3>Faculties</h3>
		<div id="all-faculty">
			<%@faculties.each do |faculty|%>
			<div class="checkbox">
				<input type="checkbox" class="faculty" value="<%=faculty.id%>"> <%=faculty.title%>
			</div>
			<%end%>
		</div>
	</div>
	<div class="col-xs-9">
		<h3>Listing courses</h3>
		 <%=render "courses", :courses => @courses%>
	</div>
</div>





