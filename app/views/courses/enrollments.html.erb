<div class="row">
	<div class="col-xs-4">
		<h3>My tests</h3>
		<div id="all-faculty">
			<%@results.each do |result|%>
			<div>
				<%=result.course.title%>
				<%=result.exam.name%>
				<%=(result.mark/result.exam.full_mark) * 100%>
			</div>
			<%end%>
		</div>
	</div>
	<div class="col-xs-8">
		<h3>My enrollments</h3>
		 <%=render "courses", :courses => @courses%>
	</div>
</div>
